<%= form_for(@contractor) do |f| %>
    <%= f.label :business_name %>
    <%= f.text_field :business_name %><br>
    <%= f.label :license_number %>
    <%= f.number_field :license_number %><br>
    <%= f.fields_for :users do |user| %>
        <%= render partial: 'users/form', locals: {user: user} %>
    <% end %>
    <%= f.submit "Sign Up" %>
<% end %>